<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<AnchorPane prefHeight="970.0" prefWidth="202.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.openjfx.ledicom.controllers.employee.EmployeeManagementMController">
   <children>
      <VBox layoutX="6.0" layoutY="14.0">
         <children>
            <HBox prefWidth="190.0">
               <children>
                  <FontIcon iconColor="#070090" iconLiteral="remixal-group-2-fill" iconSize="25" onMouseClicked="#employeeAll">
                     <HBox.margin>
                        <Insets left="1.0" top="5.0" />
                     </HBox.margin>
                  </FontIcon>
                  <Button mnemonicParsing="false" onAction="#employeeAll" prefHeight="35.0" prefWidth="160.0" stylesheets="@../../css/style.css" text="Все сотрудники">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <styleClass>
                        <String fx:value="blue" />
                        <String fx:value="button-default" />
                        <String fx:value="font-small" />
                     </styleClass>
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="35.0" prefWidth="190.0">
               <children>
                  <FontIcon iconColor="#070090" iconLiteral="remixal-book-3-fill" iconSize="25" onMouseClicked="#employeeCourses">
                     <HBox.margin>
                        <Insets left="1.0" top="5.0" />
                     </HBox.margin>
                  </FontIcon>
                  <Button mnemonicParsing="false" onAction="#employeeCourses" prefHeight="35.0" prefWidth="160.0" stylesheets="@../../css/style.css" text="Курсы">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <styleClass>
                        <String fx:value="blue" />
                        <String fx:value="button-default" />
                        <String fx:value="font-small" />
                     </styleClass>
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="35.0" prefWidth="190.0">
               <children>
                  <FontIcon iconColor="#070090" iconLiteral="remixmz-user-add-fill" iconSize="25" onMouseClicked="#employeeAdd">
                     <HBox.margin>
                        <Insets left="1.0" top="5.0" />
                     </HBox.margin>
                  </FontIcon>
                  <Button mnemonicParsing="false" onAction="#employeeAdd" prefHeight="35.0" prefWidth="160.0" stylesheets="@../../css/style.css" text="Добавить сотрудника">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                     <styleClass>
                        <String fx:value="blue" />
                        <String fx:value="button-default" />
                        <String fx:value="font-small" />
                     </styleClass>
                     <HBox.margin>
                        <Insets left="8.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
